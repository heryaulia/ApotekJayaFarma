<odoo>

    <!-- Report Daftar Supplier XLSX-->
    <record id="report_supplier_xlsx" model="ir.actions.report">
        <field name="name">Daftar Supplier Excel</field>
        <field name="model">apotek.supplier</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">apotek.report_supplier_xlsx</field>
